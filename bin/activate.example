##
# Config section for activating shell enviroments.
##

# Set secret key.
secret_key_base=

# Set secret key for Devise.
devise_secret_key_base=

##
# End of config section.
##


if [ -z "${SECRET_KEY_BASE}" ]; then
    export SECRET_KEY_BASE=${secret_key_base}
fi

if [ -z "${DEVISE_SECRET_KEY_BASE}" ]; then
    export DEVISE_SECRET_KEY_BASE=${devise_secret_key_base}
fi

app_root=$(dirname "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )")

export DAP_ROOT_PATH=${app_root}
export RAILS_ENV=production
export DAP_QUEUE_NAME=dap-dev
export REDIS_URL=redis://localhost:6379/12
export DISABLE_SPRING=1
export PATH=${app_root}/ruby/bin:${PATH}
